   "source": [
    "## Adding Bullets to Wiki Markup"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 62,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Before: \n",
      "Lists of animals\n",
      "Lists of aquarium life\n",
      "Lists of biologists by author abbreviation\n",
      "Lists of cultivars\n",
      "\n",
      "After: \n",
      "* Lists of animals\n",
      "* Lists of aquarium life\n",
      "* Lists of biologists by author abbreviation\n",
      "* Lists of cultivars\n"
     ]
    }
   ],
   "source": [
    "import pyperclip\n",
    "\n",
    "text = pyperclip.paste()\n",
    "print(f\"Before: \\n{text}\")\n",
    "\n",
    "lines = text.split(\"\\n\")\n",
    "for i in range(len(lines)):\n",
    "    lines[i] = f\"* {lines[i]}\"\n",
    "\n",
    "text = \"\\n\".join(lines)\n",
    "print(f\"\\nAfter: \\n{text}\")\n",
    "\n",
    "pyperclip.copy(text)"
   ]
  }
 ],
